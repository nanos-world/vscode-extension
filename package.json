{
	"name": "nanos-world",
	"displayName": "nanos world Lua",
	"description": "nanos world support to sumneko's Lua language server",
	"version": "1.1.0",
	"publisher": "go-horse-studios",
	"repository": "https://github.com/nanos-world/vscode-extension",
	"icon": "favicon.ico",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:lua"
	],
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.61.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^11.3.0",
		"typescript": "^4.4.3"
	},
	"extensionDependencies": [
		"sumneko.lua"
	],
	"dependencies": {
		"axios": "^1.13.6"
	}
}
